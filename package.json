{
	"$schema": "https://json.schemastore.org/package",
	"name": "clickup.js",
	"version": "3.0.4",
	"description": "A Node.js wrapper for the Clickup API",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"files": [
		"dist"
	],
	"author": "@ComfortablyCoding",
	"repository": {
		"type": "git",
		"url": "https://github.com/ComfortablyCoding/clickup.js.git"
	},
	"homepage": "https://github.com/ComfortablyCoding/clickup.js#readme",
	"bugs": {
		"url": "https://github.com/ComfortablyCoding/clickup.js/issues"
	},
	"scripts": {
		"build": "unbuild",
		"test": "vitest",
		"lint": "eslint . --fix",
		"format": "prettier . --write"
	},
	"dependencies": {
		"got": "^14.3.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.3.0",
		"eslint": "^9.3.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.3.0",
		"prettier": "^3.2.5",
		"unbuild": "^2.0.0",
		"vitest": "^1.6.0"
	},
	"keywords": [
		"clickup",
		"clickupjs",
		"clickup-js",
		"clickup.js",
		"api",
		"wrapper",
		"wrapper-api"
	]
}
